<Window x:Class="Parcial2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parcial2"
        mc:Ignorable="d"
        Title="Inicio de Sesión" Height="250" Width="400">
    <Grid>
        <Canvas >
            <ComboBox x:Name ="cmbUsers" Canvas.Left="152" Canvas.Top="32" Width="217" SelectionChanged="CmbUsers_SelectionChanged"/>
            <TextBox x:Name="txtPassword" Height="23" Canvas.Left="152" TextWrapping="Wrap" Canvas.Top="88" Width="216"/>
            <Label Content="Nombre de usuario" Height="22" Canvas.Left="23" Canvas.Top="32" Width="148"/>
            <Label Content="Contraseña" Height="27" Canvas.Left="23" Canvas.Top="84" Width="148"/>
            <Button x:Name="btnBoton" Content="Comenzar" Canvas.Left="244" Canvas.Top="147" Width="75" Click="BtnBoton_Click"/>
        </Canvas>
    </Grid>
</Window>
